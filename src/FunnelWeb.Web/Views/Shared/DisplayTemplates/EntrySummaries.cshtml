@model FunnelWeb.Repositories.PagedResult<EntrySummary>
@if (!Model.Any())
{
    <p>No posts have been published yet.</p>
	if (User.Identity.IsAuthenticated)
    {
    <p>
        Use the @Html.AdminActionLink("New Post", "Edit", "WikiAdmin") link at the top of
        the page to create a post.</p>
    }
}
else
{
    <div class="post-list col-lg-8 blogpost-list">
        @foreach (var entry in Model)
        {
            @Html.DisplayFor(x => entry)
        }
    </div>
}